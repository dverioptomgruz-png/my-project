// ===== A/B Testing Models =====

model ABExperiment {
  id                   String      @id @default(uuid())
  userId               String
  projectId            String?
  name                 String
  category             String
  baseTitle            String
  baseDescription      String
  basePrice            Float
  baseImages           String[]    @default([])
  duration             Int         // days
  rotationInterval     Int         // hours
  status               String      @default("draft") // draft, running, stopped, completed
  currentVariantIndex  Int?        @default(0)
  currentAvitoItemId   String?
  startedAt            DateTime?
  stoppedAt            DateTime?
  lastRotatedAt        DateTime?
  createdAt            DateTime    @default(now())
  updatedAt            DateTime    @updatedAt

  variants             ABVariant[]

  @@schema("public")
}

model ABVariant {
  id           String      @id @default(uuid())
  experimentId String
  name         String
  index        Int
  title        String
  description  String
  price        Float
  images       String[]    @default([])
  views        Int         @default(0)
  contacts     Int         @default(0)
  favorites    Int         @default(0)
  avitoItemId  String?
  publishedAt  DateTime?
  createdAt    DateTime    @default(now())
  updatedAt    DateTime    @updatedAt

  experiment   ABExperiment @relation(fields: [experimentId], references: [id], onDelete: Cascade)

  @@schema("public")
}

// ===== A/B Campaign (multi-SKU) =====

model ABCampaign {
  id                String         @id @default(uuid())
  userId            String
  name              String         // "Тест кастрюль, сковородок, ковшиков"
  status            String         @default("draft") // draft, preparing, running, stopped, completed
  crmSource         String?        // "moysklad" | "bitrix" | "amocrm" | null
  crmConnectionId   String?        // ID подключения к CRM
  yandexDiskUrl     String?        // Общая ссылка на Яндекс.Диск с фото
  category          String         // Категория Авито
  testStrategy      String         @default("titles") // titles, photos, prices, descriptions, all
  rotationDays      Int            @default(5) // 3-7 дней между ротациями
  totalSKUs         Int            @default(0)
  completedSKUs     Int            @default(0)
  startedAt         DateTime?
  stoppedAt         DateTime?
  createdAt         DateTime       @default(now())
  updatedAt         DateTime       @updatedAt

  products          ABProduct[]

  @@schema("public")
}

model ABProduct {
  id                String         @id @default(uuid())
  campaignId        String
  experimentId      String?        // Linked ABExperiment
  crmProductId      String?        // ID товара в CRM
  sku               String?
  name              String         // Название товара из CRM
  description       String?        // Описание из CRM
  price             Float          @default(0)
  costPrice         Float?         // Себестоимость из CRM
  stock             Int?           // Остаток на складе
  category          String?
  brand             String?
  attributes        Json?          // Доп. характеристики из CRM
  imageFolder       String?        // Подпапка на Яндекс.Диске
  status            String         @default("pending") // pending, testing, winner_found, completed
  winnerVariantId   String?
  createdAt         DateTime       @default(now())
  updatedAt         DateTime       @updatedAt

  campaign          ABCampaign     @relation(fields: [campaignId], references: [id], onDelete: Cascade)

  @@schema("public")
}
